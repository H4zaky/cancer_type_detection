%amostra(ID, CPW, AW, RW, PW, TW, SM, SW, FDS, TS, TM, CS, PS, SS, PM).
amostra(1, 0.448, 0.137, -0.311, 0.04, -0.821, 2.929, -1.396, -0.935, -0.522, -0.907, -0.429, -0.072, -0.486, 0.184).

diagnostico(ID, D) :-
    amostra(ID, CPW, AW, _, _, TW, _, _, _, _, _, _, _, _, _),
    (  
    	CPW =< 0.488,
        AW =< 0.137,
        TW =< -0.821 ->  
    	D = 'B' 
    ;   
    	D = 'M'
    ).
